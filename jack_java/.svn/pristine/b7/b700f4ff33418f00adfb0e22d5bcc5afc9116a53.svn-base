package com.java.programming.knowledge;

public class Switch {

	public static void main(String[] args) {
		int score = 90;

		switch (score) {

		case 90:
			System.out.println("very good");
			break;

		case 60:
			System.out.println("very good");
			break;

		case 40:
			System.out.println("very good");
			break;

		default:
			System.out.println("very good");
			break;
		}
	}

}
