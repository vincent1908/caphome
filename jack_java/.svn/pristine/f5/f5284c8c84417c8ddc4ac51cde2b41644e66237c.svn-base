package com.java.programming.knowledge;

public class StaticKeyword {

	public static void main(String[] args) {
		
		int z= StaticMethod.x;
		System.out.println(z);// Accessing static variable directly using class
		
		System.out.println(StaticMethod.DoSomething("static method --- accesed directly via class -- need not create intsance of class"));

		StaticMethod s1 = new StaticMethod();
		
		System.out.println(s1.DoSomethingElse("nonstatic method -- accesed via instance"));
		
		int q = s1.y;
		
		System.out.println(q);// Accessing non static variable using instance
	}

}
