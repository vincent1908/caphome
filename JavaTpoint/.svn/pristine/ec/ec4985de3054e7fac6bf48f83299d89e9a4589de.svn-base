package lnt;

class Box {
double width;
double height;
double depth;
}

public class BoxDemo {

	public static void main(String[] args) {
		
		Box mybox1 = new Box();
		Box mybox2 = new Box();
		
		double vol1;
		double vol2;
		
		mybox1.width = 15;
		mybox1.height = 20;
		mybox1.depth = 15;
		
		mybox2.width = 10;
		mybox2.height = 20;
		mybox2.depth = 15;
		
		
		vol1 = mybox1.width * mybox1.depth * mybox1.depth;
		vol2 = mybox2.width * mybox2.depth * mybox2.depth;
		
		System.out.println("volume is - " +vol1);
		System.out.println("volume is - " +vol2);
		
		
		

	}

}
