package lnt;


public class TwoMthd {
	
	int r;
	String n;
	
	void insertRecord(int rollno,String name){ //method
		
		r = rollno;
		n = name;
		
	}
	
	void displayInformation(){ //method
	System.out.println(r+ "" +n);	
	}

	public static void main(String[] args) {
		
		TwoMthd s1 = new TwoMthd();
		TwoMthd s2 = new TwoMthd();
		
		s1.insertRecord(1, "nano");
		s2.insertRecord(2, "nano1");
		
		s1.displayInformation();
		s2.displayInformation();
		}

}
