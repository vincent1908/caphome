public class Class10 {
	
	static boolean a = true;
	static boolean b = false;
	static int c = 1000;
	static int d = 2000;
	static int e = 3000;
	
	public static void main(String[] args) {

		
		System.out.println(a&&b);
		System.out.println(a||b);
		
