package com.java.programming.knowledge;

public class IfElse {

	public static void main(String[] args) {

		// if - else with relational operators
		
		int u = 20;
		
		if (u==20){
			System.out.println("u==10");
		}
		else{
			System.out.println("u!=10");
		}
		
		/*
		 == equal to
		 != not equal to
		 < lesser than
		 > greater than
		 <= lesser than or equal to
		 >= greater than or equal to
		 */
		
		
	}

}
